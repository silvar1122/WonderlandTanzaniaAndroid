<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:orientation="vertical">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:scaleType="fitXY"
        android:foreground="#66000000"
        android:src="@drawable/kilimanjaro"
        android:id="@+id/img_nature"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/grey"
        android:layout_marginBottom="5dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:text="Home"
        android:id="@+id/tv_home"
        android:drawablePadding="8dp"
        android:drawableLeft="@drawable/home"
        android:textColor="@color/grey"
        android:textSize="14sp"
        android:paddingStart="10dp"


        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/grey"
        android:layout_marginBottom="5dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:text="NewsRoom"
        android:id="@+id/tv_newsroom"
        android:drawablePadding="8dp"
        android:drawableLeft="@drawable/internet"
        android:textColor="@color/grey"
        android:textSize="14sp"
        android:layout_marginBottom="5dp"
        android:paddingStart="10dp"

        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/grey"
        android:layout_marginBottom="5dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:text="Column"
        android:id="@+id/tv_column"
        android:drawablePadding="8dp"
        android:textColor="@color/grey"
        android:textSize="14sp"
        android:layout_marginBottom="5dp"
        android:drawableLeft="@drawable/book"
        android:paddingStart="10dp"

        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/grey"
        android:layout_marginBottom="5dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:text="Photo Repotage"
        android:textColor="@color/grey"
        android:drawablePadding="8dp"
        android:textSize="14sp"
        android:id="@+id/tv_photo_repotage"
        android:layout_marginBottom="5dp"
        android:drawableLeft="@drawable/camera"
        android:paddingStart="10dp"

        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/grey"
        android:layout_marginBottom="5dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:text="Library"
        android:id="@+id/tv_library"
        android:layout_marginBottom="5dp"
        android:drawablePadding="8dp"
        android:drawableLeft="@drawable/books"
        android:textColor="@color/grey"
        android:textSize="14sp"

        android:paddingStart="10dp"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/grey"
        android:layout_marginBottom="5dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingVertical="12dp"
        android:text="About Us"
        android:id="@+id/tv_about_us"
        android:layout_marginBottom="5dp"
        android:drawablePadding="8dp"
        android:textColor="@color/grey"
        android:textSize="14sp"
        android:backgroundTint="@color/white"
        android:drawableLeft="@drawable/user"
        android:paddingStart="10dp"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/grey"
        android:layout_marginBottom="5dp"/>








</androidx.appcompat.widget.LinearLayoutCompat>